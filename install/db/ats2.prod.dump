#!/bin/sh

mysqldump -h stat.mcn.ru -u user -ppassword \
--single-transaction \
--routines \
--ignore-table=ats2.a_connect \
ats2 \
> /vagrant/install/db/dump/ats2.sql

mysqldump -h stat.mcn.ru -u user -ppassword \
--no-data \
--single-transaction \
ats2 \
a_connect \
>> /vagrant/install/db/dump/ats2.sql

