{
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "dev-master#492aeb7",
        "welltime/graylog-target": "dev-master#4ad58f3",
        "aferrandini/phpqrcode": "1.0.1",
        "phpoffice/phpexcel": "1.8.1",
        "kartik-v/yii2-grid": "3.0.2",
        "kartik-v/yii2-editable": "1.7.3",
        "kartik-v/yii2-widgets": "dev-master#b7eb849",
        "kartik-v/yii2-mpdf": "dev-master#873a4f1",
        "kartik-v/yii2-builder": "1.6.1",
        "kartik-v/yii2-datecontrol": "1.9.3",
        "kartik-v/yii2-date-range": "1.6.5",
        "tinymce/tinymce": "4.3.1",
        "smarty/smarty": "3.1.27",
        "yiisoft/yii2-swiftmailer": "2.0.4",
        "unclead/yii2-multiple-input": "1.2.8"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "2.0.4",
        "yiisoft/yii2-faker": "dev-master#66432de"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "writable": [
            "runtime",
            "web/assets"
        ],
        "executable": [
            "yii"
        ],
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
